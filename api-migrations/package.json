{
  "name": "directus-extension-api-migrations",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "index.js",
    "source": "src/index.ts",
    "host": "9.22.4"
  },
  "scripts": {
    "cleanup": "rimraf ./*.js ./*.js.map ./utils ./migrations ./node_modules",
    "build": "pnpm compile && directus-extension build",
    "dev": "directus-extension build -w --sourcemap --no-minify",
    "compile": "tsc --project ./tsconfig.json --module CommonJS --outDir ./"
  },
  "dependencies": {
    "@directus/extensions-sdk": "9.22.4",
    "@directus/shared": "9.22.3",
    "@types/fs-extra": "^9.0.13",
    "axios": "1.2.1",
    "knex": "2.3.0",
    "pino": "8.8.0",
    "ts-retry": "3.1.2",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@types/express": "4.17.13",
    "@types/express-serve-static-core": "^4.17.32",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "^5.0.8",
    "create-directus-extension": "9.22.3",
    "cypress": "^12.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-junit": "^14.0.1",
    "mysql": "^2.18.1",
    "pino": "8.8.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "syncpack": "^8.4.11",
    "ts-jest": "^27.1.5",
    "tsc-files": "^1.1.3",
    "vue-eslint-parser": "^9.1.0",
    "wait-on": "^6.0.1"
  }
}
